function work {
    export WORK=$1
    export TILLER_NAMESPACE=$1
    [[ ! $PWD == /Workspace/_$WORK/* ]] &&  cd /Workspace/_$WORK
    if [[ -f ~/env.d/$WORK ]]
    then
        source ~/env.d/$WORK
    fi
    export ANSIBLE_VAULT_PASSWORD_FILE=/Workspace/_$WORK/ansiblevault.txt
    find /Workspace/Keys -name  "$WORK.*.pem" -exec ssh-add {} + 2>/dev/null || return
}
